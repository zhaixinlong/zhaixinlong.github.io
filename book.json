{
  "description": "用于演示如何创建一个自己的Gitbook", 
  "links": {
    "sidebar": {
      "主页": "https://zhaixinlong.github.io"
    }
  }, 
  "author": "zhaixl", 
  "title": "Gitbook", 
  "gitbook": "3.2.3", 
  "language": "zh-hans", 
  "plugins": [
    "theme-comscore", 
    "-lunr", 
    "-search", 
    "search-plus", 
    "disqus", 
    "-highlight", 
    "prism", 
    "prism-themes", 
    "github-buttons", 
    "splitter", 
    "-sharing", 
    "sharing-plus", 
    "tbfed-pagefooter", 
    "expandable-chapters-small", 
    "ga", 
    "donate", 
    "sitemap-general", 
    "copy-code-button", 
    "callouts", 
    "toolbar-button"
  ], 
  "pluginsConfig": {
    "toolbar-button": {
      "url": "https://zhaixinlong.github.io", 
      "icon": "fa-file-pdf-o", 
      "label": "下载PDF"
    }, 
    "sitemap-general": {
      "prefix": "https://zhaixinlong.github.io"
    }, 
    "sharing": {
      "qq": true, 
      "douban": false, 
      "all": [
        "douban", 
        "facebook", 
        "google", 
        "instapaper", 
        "line", 
        "linkedin", 
        "messenger", 
        "pocket", 
        "qq", 
        "qzone", 
        "stumbleupon", 
        "twitter", 
        "viber", 
        "vk", 
        "weibo", 
        "whatsapp"
      ], 
      "google": false, 
      "stumbleupon": false, 
      "hatenaBookmark": false, 
      "twitter": true, 
      "vk": false, 
      "linkedin": false, 
      "instapaper": false, 
      "pocket": false, 
      "weibo": true, 
      "viber": false, 
      "facebook": true, 
      "qzone": false, 
      "messenger": false, 
      "line": false, 
      "whatsapp": false
    }, 
    "tbfed-pagefooter": {
      "modify_format": "YYYY-MM-DD HH:mm:ss", 
      "modify_label": "该文件修订时间：", 
      "copyright": "https://zhaixinlong.github.io"
    }, 
    "github-buttons": {
      "buttons": [
        {
          "repo": "zhaixinlong.github.io", 
          "count": true, 
          "type": "star", 
          "user": "zhaixinlong", 
          "size": "small"
        }, 
        {
          "count": false, 
          "width": "160", 
          "type": "follow", 
          "user": "zhaixinlong", 
          "size": "small"
        }
      ]
    }, 
    "disqus": {
      "shortName": "zhaixl"
    }, 
    "prism": {
      "css": [
        "prism-themes/themes/prism-atom-dark.css"
      ]
    }, 
    "ga": {
      "token": "UA-28297199-1"
    }, 
    "theme-default": {
      "showLevel": true
    }, 
    "donate": {
      "alipay": "", 
      "title": "", 
      "alipayText": "支付宝打赏", 
      "button": "打赏", 
      "wechatText": "微信打赏", 
      "wechat": ""
    }, 
    "callouts": {
      "showTypeInHeader": false
    }
  }, 
  "root": "./docs"
